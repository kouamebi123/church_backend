scripts/deploy-production.sh
prisma/seed.js

# Contrôleurs avec exemples
controllers/emergencyController.js

# Patterns spécifiques à ignorer
# Exemples SMTP
smtp\.example\.com
your-email@example\.com
your-app-password

# Exemples JWT
your-.*-secret
your-secure-.*-secret

# Mots de passe de test
password123
testpass123
motdepasse123

# Emails d'exemple
admin@.*-domain\.com
dev@.*-domaine\.com
admin@votre-domaine\.com

# Domaines d'exemple
your-domain\.com
votre-domaine\.com
example\.com
